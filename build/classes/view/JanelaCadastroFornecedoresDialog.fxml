<AnchorPane id="AnchorPane" prefHeight="317.0" prefWidth="498.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.JanelaCadastroFornecedoresDialogController">
